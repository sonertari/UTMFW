+static UtmfwConfig * UtmfwParse(char *args)
+{
+	char *arg;
+	char *argEnd;
+	int port;
+	UtmfwConfig *config = (UtmfwConfig *)calloc(1, sizeof(UtmfwConfig));
+
+	if (config == NULL)
+		_dpd.fatalMsg("Could not allocate configuration struct.\n");
+
+	#define MAXTOKENS 11
+	char s[512], *p, *tokens[MAXTOKENS];
+	char *last;
+	int i = 0;
+
+	snprintf(s, sizeof(s), args);
+
+	for ((p = strtok_r(s, " \t\n\r,", &last)); p;
+		(p = strtok_r(NULL, " \t\n\r,", &last))) {
+			if (i < MAXTOKENS - 1)
+					tokens[i++] = p;
+	}
+	tokens[i] = NULL;
+	
+	for (i = 0; i <= MAX_PORTS; i++) {
+		config->portsToListen[i] = -1;
+	}
+
+	if(tokens[0] && !strcasecmp("ports", tokens[0]))
+	{
+		int j = 0;
+		for (i = 1; tokens[i]; i++) {
+			port = strtol(tokens[i], &argEnd, 10);
+			if (port < 0 || port > 65535)
+			{
+				_dpd.fatalMsg("UtmfwPreproc: Invalid port %u\n", port);
+			}
+			else
+			{
+				config->portsToListen[j] = (uint16_t)port;
+				_dpd.logMsg("	Port: %u\n", config->portsToListen[j]);
+				j++;
+			}
+		}
+	}
+	else
+	{
+		_dpd.fatalMsg("UtmfwPreproc: Invalid option %s\n",
+			arg?arg:"(missing ports)");
+	}
+
+	return config;
+}
+
+void UtmfwProcess(void *pkt, void *context)
+{
+	SFSnortPacket *p = (SFSnortPacket *)pkt;
+	UtmfwConfig *config;
+	PROFILE_VARS;
+
+	DEBUG_WRAP(DebugMessage(DEBUG_UTMFW, "UtmfwProcess ENTER\n"););
+
+	sfPolicyUserPolicySet(utmfw_config, _dpd.getNapRuntimePolicy());
+	config = (UtmfwConfig *)sfPolicyUserDataGetCurrent(utmfw_config);
+	if (config == NULL)
+		return;
+
+	// preconditions - what we registered for
+	assert(IsTCP(p));
+
+	PREPROC_PROFILE_START(utmfwPerfStats);
+
+	if (p->payload && p->payload_size) {
+ 		int port_match = 0;
+		int i;
+		for (i = 0; config->portsToListen[i] != -1 && i < MAX_PORTS; i++) {
+			if (p->dst_port == config->portsToListen[i]) {
+				port_match = 1;
+				break;
+			}
+		}
+
+		if (port_match) {
+			char *sslproxy = memmem(p->payload, p->payload_size, "SSLproxy:", 9);
+			if (sslproxy) {
+// SSLproxy: [127.0.0.1]:34649,[192.168.3.24]:47286,[192.168.111.130]:443,s,soner\r\n
+// 9        + 2 + 45 + 2 + 5  + 2 + 45 + 2 + 5     + 2 + 45 + 2 + 5      + 1 + 1 + 1 + 31 + 2 = 207
+#define MAX_LINE_LEN 207
+#define MAX_IPADDR_LEN 45
+#define MAX_PORT_LEN 5
+#define MAX_USER_LEN 31
+				char line[MAX_LINE_LEN + 1];
+				char sslproxy_src_addr[MAX_IPADDR_LEN + 1];
+				char sslproxy_dst_addr[MAX_IPADDR_LEN + 1];
+				char port[MAX_PORT_LEN + 1];
+
+				int payload_rest_len = p->payload_size - (sslproxy - (char *)p->payload);
+				char *line_end = memmem(sslproxy, payload_rest_len, "\n", 1);
+				if (!line_end) {
+					_dpd.errMsg("UtmfwPreproc: Cannot find new line: %s\n", sslproxy);
+					goto out;
+				}
+
+				int line_len = line_end - sslproxy;
+				if (line_len > MAX_LINE_LEN) {
+					_dpd.errMsg("UtmfwPreproc: line_len larger than MAX_LINE_LEN: %d\n", line_len);
+					goto out;
+				}
+
+				memcpy(line, sslproxy, line_len);
+				line[line_len] = '\0';
+
+				DEBUG_WRAP(DebugMessage(DEBUG_UTMFW, "Found: %s\n", line););
+				
+				char *ip_start = strchr(line, '[') + 1;
+				char *ip_end = strchr(ip_start, ']');
+				char *port_start = strchr(ip_end, ':') + 1;
+				char *port_end = strchr(port_start, ',');
+
+				if (!ip_start || !ip_end || !port_start || !port_end) {
+					DEBUG_WRAP(DebugMessage(DEBUG_UTMFW, "Unable to find sslproxy addr\n"););
+					_dpd.errMsg("UtmfwPreproc: Unable to find sslproxy addr: %s\n", sslproxy);
+					goto out;
+				}
+
+				ip_start = strchr(port_end, '[') + 1;
+				ip_end = strchr(ip_start, ']');
+				port_start = strchr(ip_end, ':') + 1;
+				port_end = strchr(port_start, ',');
+
+				if (!ip_start || !ip_end || !port_start || !port_end) {
+					DEBUG_WRAP(DebugMessage(DEBUG_UTMFW, "Unable to find src addr\n"););
+					_dpd.errMsg("UtmfwPreproc: Unable to find src addr: %s\n", sslproxy);
+					goto out;
+				}
+
+				int addr_len = ip_end - ip_start;
+				if (addr_len > MAX_IPADDR_LEN) {
+					_dpd.errMsg("UtmfwPreproc: src addr_len larger than MAX_IPADDR_LEN: %d\n", addr_len);
+					goto out;
+				}
+
+				strncpy(sslproxy_src_addr, ip_start, addr_len);
+				sslproxy_src_addr[addr_len] = '\0';
+
+				int port_len = port_end - port_start;
+				if (port_len > MAX_PORT_LEN) {
+					_dpd.errMsg("UtmfwPreproc: src port_len larger than MAX_PORT_LEN: %d\n", port_len);
+					goto out;
+				}
+
+				strncpy(port, port_start, port_len);
+				port[port_len] = '\0';
+				int sslproxy_src_port = atoi(port);
+				
+				ip_start = strchr(port_end, '[') + 1;
+				ip_end = strchr(ip_start, ']');
+				port_start = strchr(ip_end, ':') + 1;
+				port_end = strchr(port_start, ',');
+
+				if (!ip_start || !ip_end || !port_start || !port_end) {
+					DEBUG_WRAP(DebugMessage(DEBUG_UTMFW, "Unable to find dst addr\n"););
+					_dpd.errMsg("UtmfwPreproc: Unable to find dst addr: %s\n", sslproxy);
+					goto out;
+				}
+
+				addr_len = ip_end - ip_start;
+				if (addr_len > MAX_IPADDR_LEN) {
+					_dpd.errMsg("UtmfwPreproc: dst addr_len larger than MAX_IPADDR_LEN: %d\n", addr_len);
+					goto out;
+				}
+
+				strncpy(sslproxy_dst_addr, ip_start, addr_len);
+				sslproxy_dst_addr[addr_len] = '\0';
+
+				port_len = port_end - port_start;
+				if (port_len > MAX_PORT_LEN) {
+					_dpd.errMsg("UtmfwPreproc: dst port_len larger than MAX_PORT_LEN: %d\n", port_len);
+					goto out;
+				}
+
+				strncpy(port, port_start, port_len);
+				port[port_len] = '\0';
+				int sslproxy_dst_port = atoi(port);
+
+				char *ssl_start = port_end + 1;
+				char *ssl_end = strchr(ssl_start, ',');
+				char sslproxy_user[MAX_USER_LEN + 1];
+				int ssl_len = 0;
+				if (ssl_end) {
+					// There is user info at the end
+					ssl_len = ssl_end - ssl_start;
+
+					char *user = ssl_end + 1;
+
+					int user_len = strlen(user);
+					if (user_len > MAX_USER_LEN) {
+						_dpd.errMsg("UtmfwPreproc: user_len larger than MAX_USER_LEN: %d\n", user_len);
+						goto out;
+					}
+
+					strncpy(sslproxy_user, user, user_len);
+					sslproxy_user[user_len] = '\0';
+				} else {
+					ssl_len = strlen(ssl_start);
+
+					strncpy(sslproxy_user, "Unknown", 7);
+					sslproxy_user[7] = '\0';
+				}
+
+				if (ssl_len > 1) {
+					_dpd.errMsg("UtmfwPreproc: ssl_len larger than 1: %d\n", ssl_len);
+					goto out;
+				}
+
+				int sslproxy_ssl = strchr(ssl_start, 's') ? 1 : 0;
+
+				DEBUG_WRAP(DebugMessage(DEBUG_UTMFW, "Parsed SSLproxy line: srcaddr= %s, srcport= %d, dstaddr= %s, dstport= %d, ssl=%d, user= %s\n",
+						sslproxy_src_addr, sslproxy_src_port, sslproxy_dst_addr, sslproxy_dst_port, sslproxy_ssl, sslproxy_user););
+
+				sfaddr_t *iphdt = &p->ip4h->ip_addrs->ip_src;
+				struct in6_addr *ipV4address = &iphdt->ip;
+
+				DEBUG_WRAP(DebugMessage(DEBUG_UTMFW, "ORIG src = %u.%u.%u.%u:%u\n",
+								ipV4address->s6_addr[12], ipV4address->s6_addr[13], ipV4address->s6_addr[14], ipV4address->s6_addr[15], p->src_port););
+
+				struct in_addr src_addr;
+				if (inet_pton(AF_INET, sslproxy_src_addr, &src_addr) != 1) {
+					_dpd.errMsg("UtmfwPreproc: Cannot inet_pton sslproxy_src_addr: %s\n", sslproxy);
+					goto out;
+				}
+
+				iphdt->ia32[3]= *((uint32_t *)&src_addr);
+
+				DEBUG_WRAP(DebugMessage(DEBUG_UTMFW, "NEW src = %u.%u.%u.%u:%u\n",
+								ipV4address->s6_addr[12], ipV4address->s6_addr[13], ipV4address->s6_addr[14], ipV4address->s6_addr[15], sslproxy_src_port););
+
+				iphdt = &p->ip4h->ip_addrs->ip_dst;
+				ipV4address = &iphdt->ip;
+
+				DEBUG_WRAP(DebugMessage(DEBUG_UTMFW, "ORIG dst = %u.%u.%u.%u:%u\n",
+								ipV4address->s6_addr[12], ipV4address->s6_addr[13], ipV4address->s6_addr[14], ipV4address->s6_addr[15], p->dst_port););
+
+				struct in_addr dst_addr;
+				if (inet_pton(AF_INET, sslproxy_dst_addr, &dst_addr) != 1) {
+					_dpd.errMsg("UtmfwPreproc: Cannot inet_pton sslproxy_dst_addr: %s\n", sslproxy);
+					goto out;
+				}
+
+				iphdt->ia32[3]= *((uint32_t *)&dst_addr);
+
+				DEBUG_WRAP(DebugMessage(DEBUG_UTMFW, "NEW dst = %u.%u.%u.%u:%u\n",
+								ipV4address->s6_addr[12], ipV4address->s6_addr[13], ipV4address->s6_addr[14], ipV4address->s6_addr[15], sslproxy_dst_port););
+
+				p->src_port = sslproxy_src_port;
+				p->dst_port = sslproxy_dst_port;
+
+				// Src and dst addresses updated, log alert
+				_dpd.alertAdd(GENERATOR_UTMFW, DST_PORT_MATCH, 1, 0, 3, DST_PORT_MATCH_STR, 0);
+			}
+			else {
+				DEBUG_WRAP(DebugMessage(DEBUG_UTMFW, "Cannot find SSLproxy line\n"););
+			}
+		}
+		else {
+			DEBUG_WRAP(DebugMessage(DEBUG_UTMFW, "Not a port we are listening for: src port= %u, dst port= %u\n", p->src_port, p->dst_port););
+		}
+	}
+	else {
+		DEBUG_WRAP(DebugMessage(DEBUG_UTMFW, "No payload\n"););
+	}
+out:
+	PREPROC_PROFILE_END(utmfwPerfStats);
+}
+
+#ifdef SNORT_RELOAD
+static void UtmfwReload(struct _SnortConfig *sc, char *args, void **new_config)
+{
+	tSfPolicyUserContextId utmfw_swap_config;
+	UtmfwConfig *config;
+	tSfPolicyId policy_id = _dpd.getParserPolicy(sc);
+
+	_dpd.logMsg("Utmfw dynamic preprocessor configuration reloaded\n");
+
+	utmfw_swap_config = sfPolicyConfigCreate();
+	if (utmfw_swap_config == NULL)
+		_dpd.fatalMsg("Could not allocate configuration struct.\n");
+
+	config = UtmfwParse(args);
+	sfPolicyUserPolicySet(utmfw_swap_config, policy_id);
+	sfPolicyUserDataSetCurrent(utmfw_swap_config, config);
+
+	/* Register the preprocessor function, Transport layer, ID 10000 */
+	_dpd.addPreproc(sc, UtmfwProcess, PRIORITY_TRANSPORT, PP_UTMFW, PROTO_BIT__TCP);
+
+	if (_dpd.streamAPI == NULL)
+	{
+		DynamicPreprocessorFatalMessage("Streaming & reassembly must be enabled "
+				"for utmfw preprocessor\n");
+	}
+
+	int i;
+	for (i = 0; config->portsToListen[i] != -1 && i < MAX_PORTS; i++) {
+		// @attention Register ports with session and stream, otherwise we cannot get any packets
+		_dpd.sessionAPI->enable_preproc_for_port(sc, PP_UTMFW, PROTO_BIT__TCP, config->portsToListen[i]);
+		// The SSLproxy info is in the packets from client to server only, but do we need stream anyway?
+		_dpd.streamAPI->register_reassembly_port(NULL, config->portsToListen[i], SSN_DIR_FROM_SERVER | SSN_DIR_FROM_CLIENT);
+	}
+
+	*new_config = (void *)utmfw_swap_config;
+	DEBUG_WRAP(DebugMessage(DEBUG_UTMFW, "Preprocessor: Utmfw is re-initialized\n"););
+}
+
+static int UtmfwReloadVerify(struct _SnortConfig *sc, void *swap_config)
+{
+	if (!_dpd.isPreprocEnabled(sc, PP_STREAM))
+	{
+		_dpd.errMsg("Streaming & reassembly must be enabled for utmfw preprocessor\n");
+		return -1;
+	}
+
+	return 0;
+}
+
+static int UtmfwReloadSwapPolicyFree(tSfPolicyUserContextId config, tSfPolicyId policyId, void *data)
+{
+	UtmfwConfig *policy_config = (UtmfwConfig *)data;
+
+	sfPolicyUserDataClear(config, policyId);
+	free(policy_config);
+	return 0;
+}
+
+static void * UtmfwReloadSwap(struct _SnortConfig *sc, void *swap_config)
+{
+	tSfPolicyUserContextId utmfw_swap_config = (tSfPolicyUserContextId)swap_config;
+	tSfPolicyUserContextId old_config = utmfw_config;
+
+	if (utmfw_swap_config == NULL)
+		return NULL;
+
+	utmfw_config = utmfw_swap_config;
+
+	return (void *)old_config;
+}
+
+static void UtmfwReloadSwapFree(void *data)
+{
+	tSfPolicyUserContextId config = (tSfPolicyUserContextId)data;
+
+	if (data == NULL)
+		return;
+
+	sfPolicyUserDataFreeIterate(config, UtmfwReloadSwapPolicyFree);
+	sfPolicyConfigDelete(config);
+}
+#endif
